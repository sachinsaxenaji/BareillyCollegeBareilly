<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bcb.user.bareillycollegebareilly">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".about"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Events" />
        <activity
            android:name=".course"
            android:exported="true" />
        <activity android:name=".Syllabus" />
        <activity android:name=".contact" />
        <activity
            android:name="com.bcb.user.course.Bba"
            android:label="@string/title_activity_bba"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Bca"
            android:label="@string/title_activity_bca"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Bsc"
            android:label="@string/title_activity_bsc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Ba"
            android:label="@string/title_activity_ba"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Bcom"
            android:label="@string/title_activity_bcom"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Bcomh"
            android:label="@string/title_activity_bcomh"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Med"
            android:label="@string/title_activity_med"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Pgdca"
            android:label="@string/title_activity_pgdca"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.bcb.user.course.Mlib"
            android:label="@string/title_activity_mlib"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bcb.login.SignUp" />
        <activity android:name="com.bcb.login.Slider" />
        <activity android:name="com.bcb.Doveloper"></activity>
    </application>

</manifest>